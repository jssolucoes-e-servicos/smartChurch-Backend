    "@nestjs/event-emitter": "1.3.1",
    "@nestjs/jwt": "9.0.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/passport": "9.0.0",
    "@nestjs/platform-express": "9.0.0",
    "@nestjs/platform-socket.io": "9.2.1",
    "@nestjs/schedule": "2.1.0",
    "@nestjs/serve-static": "3.0.1",
    "@nestjs/swagger": "6.1.4",
    "@nestjs/websockets": "9.2.1",
    "@prisma/client": "4.7.1",
